
/* Using Bootstrap variables for compatibility */
body {
    font-family: 'Open Sans', sans-serif;
    color: #555555;
    background-color: #f6f7f8;
    overflow-x: hidden;
}

h1, h2, h3, h4, h5, h6 {
    font-family: 'Raleway', sans-serif;
    font-weight: 700;
}

a {
    text-decoration: none;
    transition: var(--transition);
}

/*** Spinner Start ***/
#spinner {
    opacity: 0;
    visibility: hidden;
    transition: opacity .8s ease-out, visibility 0s linear .5s;
    z-index: 99999;
    background-color: rgba(255, 255, 255, 0.9);
}

#spinner.show {
    transition: opacity .8s ease-out, visibility 0s linear .0s;
    visibility: visible;
    opacity: 1;
}

#spinner .spinner-grow {
    width: 3rem;
    height: 3rem;
    animation: spinner-grow 1s linear infinite;
}

.back-to-top {
    position: fixed;
    right: 30px;
    bottom: 30px;
    display: flex;
    width: 50px;
    height: 50px;
    align-items: center;
    justify-content: center;
    transition: 0.5s;
    z-index: 99;
    box-shadow: 0 0 20px rgba(0, 0, 0, 0.1);
    opacity: 0;
    visibility: hidden;
    transform: translateY(15px);
}

.back-to-top.show {
    opacity: 1;
    visibility: visible;
    transform: translateY(0);
}

.back-to-top:hover {
    transform: translateY(-5px);
}
/*** Spinner End ***/


/*** Button Start ***/
.btn {
    font-weight: 600;
    transition: all 0.4s ease;
    border-radius: 50px;
    padding: 10px 25px;
    position: relative;
    overflow: hidden;
    z-index: 1;
}

.btn::after {
    content: '';
    position: absolute;
    bottom: 0;
    left: 0;
    width: 100%;
    height: 0;
    background-color: rgba(0, 0, 0, 0.1);
    transition: all 0.3s ease;
    z-index: -1;
}

.btn:hover::after {
    height: 100%;
}

.btn-primary {
    background-color: var(--primary-color);
    border-color: var(--primary-color);
}

.btn-primary:hover {
    background-color: var(--primary-color);
    border-color: var(--primary-color);
    transform: translateY(-3px);
    box-shadow: 0 10px 20px rgba(0, 0, 0, 0.1);
}

.btn-secondary {
    background-color: var(--secondary-color);
    border-color: var(--secondary-color);
}

.btn-secondary:hover {
    background-color: var(--secondary-color);
    border-color: var(--secondary-color);
    transform: translateY(-3px);
    box-shadow: 0 10px 20px rgba(0, 0, 0, 0.1);
}

.btn-square {
    width: 36px;
    height: 36px;
    padding: 0;
    display: inline-flex;
    align-items: center;
    justify-content: center;
    font-weight: normal;
    border-radius: 50%;
}

.btn-sm-square {
    width: 38px;
    height: 38px;
    padding: 0;
    display: inline-flex;
    align-items: center;
    justify-content: center;
    font-weight: normal;
    border-radius: 50%;
}

.btn-md-square {
    width: 48px;
    height: 48px;
    padding: 0;
    display: inline-flex;
    align-items: center;
    justify-content: center;
    font-weight: normal;
    border-radius: 50%;
}

.btn-lg-square {
    width: 58px;
    height: 58px;
    padding: 0;
    display: inline-flex;
    align-items: center;
    justify-content: center;
    font-weight: normal;
    border-radius: 50%;
}

.btn.border-secondary {
    transition: all 0.4s ease;
    border: 2px solid var(--secondary-color);
}

.btn.border-secondary:hover {
    background: var(--secondary-color) !important;
    color: var(--light-color) !important;
    transform: translateY(-3px);
    box-shadow: 0 10px 20px rgba(0, 0, 0, 0.1);
}

.btn-outline-primary {
    color: var(--primary-color);
    border-color: var(--primary-color);
}

.btn-outline-primary:hover {
    background-color: var(--primary-color);
    border-color: var(--primary-color);
    color: white;
}

.btn-outline-secondary {
    color: var(--secondary-color);
    border-color: var(--secondary-color);
}

.btn-outline-secondary:hover {
    background-color: var(--secondary-color);
    border-color: var(--secondary-color);
    color: white;
}

/*** Topbar Start ***/
.fixed-top {
    transition: all 0.5s ease;
    background: var(--light-color);
    border: 0;
    box-shadow: 0 0 30px rgba(0, 0, 0, 0.05);
}

.topbar {
    padding: 15px 20px;
    border-radius: 0 0 30px 30px;
    background: linear-gradient(135deg, var(--primary-color), #6ea306);
}

.topbar .top-info {
    font-size: 14px;
    line-height: 1.2;
    letter-spacing: 0.5px;
    display: flex;
    align-items: center;
}

.topbar .top-info i {
    margin-right: 8px;
    font-size: 16px;
    height: 30px;
    width: 30px;
    background: rgba(255, 255, 255, 0.1);
    display: inline-flex;
    align-items: center;
    justify-content: center;
    border-radius: 50%;
    transition: all 0.3s ease;
}

.topbar .top-info a:hover i {
    background: var(--secondary-color);
    transform: rotateY(180deg);
}

.topbar .top-link {
    font-size: 14px;
    line-height: 1.2;
    letter-spacing: 0.5px;
    display: flex;
    align-items: center;
}

.topbar .top-link a {
    letter-spacing: 0.5px;
    position: relative;
    margin: 0 10px;
    transition: all 0.3s ease;
}

.topbar .top-link a:after {
    content: '';
    position: absolute;
    bottom: -5px;
    left: 0;
    width: 0;
    height: 2px;
    background: var(--secondary-color);
    transition: all 0.3s ease;
}

.topbar .top-link a:hover:after {
    width: 100%;
}

.topbar .top-link a small:hover {
    color: var(--secondary-color) !important;
}

.topbar .top-link a small:hover i {
    color: var(--primary-color) !important;
}
/*** Topbar End ***/

/*** Navbar Start ***/
.navbar {
    height: 90px;
    padding: 0;
    transition: all 0.4s ease;
}

.navbar-brand {
    font-family: 'Raleway', sans-serif;
    font-weight: 800;
    transition: all 0.3s ease;
}

.navbar-brand:hover {
    transform: scale(1.05);
}

.navbar .navbar-nav .nav-link {
    padding: 10px 18px;
    font-size: 16px;
    font-weight: 600;
    color: var(--dark-color);
    transition: all 0.4s ease;
    position: relative;
}

.navbar .navbar-nav .nav-link:after {
    content: '';
    position: absolute;
    bottom: 5px;
    left: 50%;
    transform: translateX(-50%);
    width: 0;
    height: 2px;
    background: var(--primary-color);
    transition: all 0.3s ease;
}

.navbar .navbar-nav .nav-link:hover:after,
.navbar .navbar-nav .nav-link.active:after {
    width: 60%;
}

.navbar .navbar-nav .nav-link:hover,
.navbar .navbar-nav .nav-link.active {
    color: var(--primary-color);
}

.navbar .dropdown-toggle::after {
    border: none;
    content: "\f107";
    font-family: "Font Awesome 5 Free";
    font-weight: 700;
    vertical-align: middle;
    margin-left: 8px;
    transition: all 0.3s ease;
}

.navbar .dropdown-toggle:hover::after {
    transform: rotate(180deg);
}

@media (min-width: 1200px) {
    .navbar .nav-item .dropdown-menu {
        display: block;
        visibility: hidden;
        top: 120%;
        transform: rotateX(-75deg);
        transform-origin: 0% 0%;
        border: 0;
        transition: all 0.5s ease;
        opacity: 0;
        border-radius: 10px;
        box-shadow: 0 10px 30px rgba(0, 0, 0, 0.1);
    }
}

.dropdown .dropdown-menu {
    padding: 10px 0;
}

.dropdown .dropdown-menu a {
    padding: 8px 20px;
    font-weight: 500;
    transition: all 0.3s ease;
}

.dropdown .dropdown-menu a:hover {
    background: var(--secondary-color);
    color: white;
    padding-left: 25px;
}

.navbar .nav-item:hover .dropdown-menu {
    transform: rotateX(0deg);
    visibility: visible;
    background: white !important;
    border-radius: 10px !important;
    transition: all 0.5s ease;
    opacity: 1;
    top: 100%;
}

#searchModal .modal-content {
    background: rgba(255, 255, 255, 0.95);
    border-radius: 20px;
}

#searchModal .modal-header {
    border-bottom: none;
}

#searchModal .modal-body .input-group {
    box-shadow: 0 5px 20px rgba(0, 0, 0, 0.1);
    border-radius: 50px;
    overflow: hidden;
}

#searchModal .modal-body .input-group input {
    border-radius: 50px 0 0 50px;
    border: none;
    padding: 15px 25px;
    font-size: 16px;
}

#searchModal .modal-body .input-group .input-group-text {
    border-radius: 0 50px 50px 0;
    border: none;
    background: var(--primary-color);
    color: white;
    padding: 0 25px;
}

.navbar-toggler {
    border: none;
    font-size: 22px;
}

.navbar-toggler:focus {
    box-shadow: none;
}

/* Shopping cart icon animation */
.position-relative.me-4.my-auto i.fa-shopping-bag {
    transition: all 0.3s ease;
}

.position-relative.me-4.my-auto:hover i.fa-shopping-bag {
    transform: translateY(-5px);
    color: var(--primary-color);
}

/* User icon animation */
.my-auto i.fa-user {
    transition: all 0.3s ease;
}

.my-auto:hover i.fa-user {
    transform: scale(1.1);
    color: var(--primary-color);
}
/*** Navbar End ***/

/*** Hero Header ***/
.hero-header {
    background: linear-gradient(rgba(129, 196, 8, 0.05), rgba(255, 181, 36, 0.05)), url(../img/hero-img.jpg);
    background-position: center center;
    background-repeat: no-repeat;
    background-size: cover;
    position: relative;
    overflow: hidden;
}

.hero-header::before {
    content: '';
    position: absolute;
    top: 0;
    left: 0;
    width: 100%;
    height: 100%;
    background: url(../img/pattern.png);
    opacity: 0.05;
    z-index: 0;
}

.hero-header h4 {
    font-weight: 700;
    letter-spacing: 2px;
    text-transform: uppercase;
    animation: fadeInDown 1s both;
}

.hero-header h1 {
    font-weight: 800;
    text-shadow: 0 2px 5px rgba(0, 0, 0, 0.1);
    animation: fadeInUp 1s both 0.3s;
}

.hero-header .position-relative {
    animation: fadeIn 1s both 0.6s;
}

.hero-header .form-control {
    height: 60px;
    font-size: 16px;
    box-shadow: 0 5px 30px rgba(0, 0, 0, 0.1);
}

.hero-header .btn {
    height: 60px !important;
    font-size: 18px;
    font-weight: 700;
    box-shadow: 0 5px 30px rgba(0, 0, 0, 0.1);
}

.carousel-item {
    position: relative;
    border-radius: 20px;
    overflow: hidden;
    box-shadow: 0 15px 30px rgba(0, 0, 0, 0.1);
    transition: all 0.5s ease;
}

.carousel-item img {
    transition: all 0.5s ease;
    transform: scale(1.01);
}

.carousel-item:hover img {
    transform: scale(1.1);
}

.carousel-item a {
    position: absolute;
    top: 50%;
    left: 50%;
    transform: translate(-50%, -50%);
    font-size: 22px;
    font-weight: 700;
    background: linear-gradient(rgba(255, 181, 36, 0.8), rgba(255, 181, 36, 0.8));
    padding: 12px 30px;
    border-radius: 50px;
    box-shadow: 0 5px 20px rgba(0, 0, 0, 0.2);
    transition: all 0.3s ease;
}

.carousel-item a:hover {
    background: linear-gradient(rgba(129, 196, 8, 0.8), rgba(129, 196, 8, 0.8));
    transform: translate(-50%, -50%) scale(1.1);
}

.carousel-control-next,
.carousel-control-prev {
    width: 50px;
    height: 50px;
    border-radius: 50%;
    border: 2px solid white;
    background: var(--primary-color);
    position: absolute;
    top: 50%;
    transform: translateY(-50%);
    opacity: 0.8;
    transition: all 0.3s ease;
}

.carousel-control-next:hover,
.carousel-control-prev:hover {
    background: var(--secondary-color);
    opacity: 1;
    transform: translateY(-50%) scale(1.1);
}

.carousel-control-next {
    margin-right: 20px;
}

.carousel-control-prev {
    margin-left: 20px;
}

.page-header {
    position: relative;
    background: linear-gradient(rgba(0, 0, 0, 0.5), rgba(0, 0, 0, 0.5)), url(../img/cart-page-header-img.jpg);
    background-position: center center;
    background-repeat: no-repeat;
    background-size: cover;
    padding: 120px 0;
    text-align: center;
}

.page-header h1 {
    color: white;
    font-size: 48px;
    font-weight: 800;
    text-shadow: 0 2px 10px rgba(0, 0, 0, 0.3);
}

.page-header .breadcrumb {
    background: transparent;
    display: inline-flex;
    padding: 15px 30px;
    border-radius: 50px;
    background: rgba(255, 255, 255, 0.1);
    backdrop-filter: blur(5px);
}

.page-header .breadcrumb-item {
    color: white;
    font-weight: 500;
}

.page-header .breadcrumb-item.active {
    color: var(--secondary-color);
}

.page-header .breadcrumb-item+.breadcrumb-item::before {
    color: white;
}

@media (min-width: 992px) {
    .hero-header,
    .page-header {
        margin-top: 140px !important;
    }
}

@media (max-width: 992px) {
    .hero-header,
    .page-header {
        margin-top: 90px !important;
    }
    
    .hero-header h1 {
        font-size: 36px !important;
    }
}

@keyframes fadeInDown {
    from {
        opacity: 0;
        transform: translateY(-20px);
    }
    to {
        opacity: 1;
        transform: translateY(0);
    }
}

@keyframes fadeInUp {
    from {
        opacity: 0;
        transform: translateY(20px);
    }
    to {
        opacity: 1;
        transform: translateY(0);
    }
}

@keyframes fadeIn {
    from {
        opacity: 0;
    }
    to {
        opacity: 1;
    }
}
/*** Hero Header end ***/


/*** Features Start ***/
.featurs {
    position: relative;
    z-index: 1;
}

.featurs::before {
    content: '';
    position: absolute;
    top: 0;
    left: 0;
    width: 100%;
    height: 100%;
    background: url(../img/pattern.png);
    opacity: 0.05;
    z-index: -1;
}

.featurs .featurs-item {
    transition: all 0.4s ease;
    border-radius: 15px;
    overflow: hidden;
    box-shadow: 0 10px 30px rgba(0, 0, 0, 0.05);
}

.featurs .featurs-item:hover {
    transform: translateY(-10px);
    box-shadow: 0 15px 40px rgba(0, 0, 0, 0.1);
}

.featurs .featurs-item .featurs-icon {
    position: relative;
    width: 120px;
    height: 120px;
    margin: 0 auto;
    display: flex;
    align-items: center;
    justify-content: center;
    border-radius: 50%;
    background: var(--secondary-color);
    margin-bottom: 30px;
    transition: all 0.5s ease;
}

.featurs .featurs-item:hover .featurs-icon {
    transform: rotateY(180deg);
}

.featurs .featurs-item .featurs-icon i {
    font-size: 50px;
    color: white;
    transition: all 0.5s ease;
}

.featurs .featurs-item:hover .featurs-icon i {
    transform: rotateY(180deg);
}

.featurs .featurs-item .featurs-icon::after {
    content: "";
    width: 35px;
    height: 35px;
    background: var(--secondary-color);
    position: absolute;
    bottom: -10px;
    transform: translate(-50%) rotate(45deg);
    z-index: -1;
}

.featurs .featurs-item .featurs-content h5 {
    font-size: 22px;
    font-weight: 700;
    margin-bottom: 15px;
    transition: all 0.3s ease;
}

.featurs .featurs-item:hover .featurs-content h5 {
    color: var(--primary-color);
}

.featurs .featurs-item .featurs-content p {
    font-size: 16px;
    margin-bottom: 0;
    color: var(--text-color);
}
/*** Features End ***/


/*** service Start ***/
.service .service-item .service-content {
    position: relative;
    width: 250px; 
    height: 130px; 
    top: -50%; 
    left: 50%; 
    transform: translate(-50%, -50%);
}

/*** service End ***/


/*** Fruits Start ***/
.fruite {
    position: relative;
    z-index: 1;
}

.fruite h1 {
    font-size: 42px;
    font-weight: 800;
    margin-bottom: 30px;
    position: relative;
    display: inline-block;
}

.fruite h1::after {
    content: '';
    position: absolute;
    bottom: -10px;
    left: 0;
    width: 50%;
    height: 3px;
    background: var(--secondary-color);
}

.fruite .tab-class {
    margin-bottom: 40px;
}

.fruite .tab-class .nav-item {
    margin: 0 5px;
}

.fruite .tab-class .nav-item a {
    border-radius: 50px;
    font-weight: 700;
    padding: 15px 25px;
    transition: all 0.4s ease;
    border: 2px solid transparent;
}

.fruite .tab-class .nav-item a:hover {
    background: rgba(255, 181, 36, 0.1) !important;
    border-color: var(--secondary-color);
}

.fruite .tab-class .nav-item a.active {
    background: var(--secondary-color) !important;
    border-color: var(--secondary-color);
}

.fruite .tab-class .nav-item a span {
    font-size: 16px;
    transition: all 0.4s ease;
}

.fruite .tab-class .nav-item a.active span {
    color: white !important; 
}

.fruite .fruite-categorie {
    padding: 20px;
    border-radius: 15px;
    background: white;
    box-shadow: 0 10px 30px rgba(0, 0, 0, 0.05);
    transition: all 0.4s ease;
}

.fruite .fruite-categorie:hover {
    transform: translateY(-10px);
    box-shadow: 0 15px 40px rgba(0, 0, 0, 0.1);
}

.fruite .fruite-categorie .fruite-name {
    line-height: 40px;
}

.fruite .fruite-categorie .fruite-name a {
    transition: all 0.4s ease;
    font-weight: 600;
    font-size: 16px;
    color: var(--dark-color);
    position: relative;
    padding-left: 25px;
}

.fruite .fruite-categorie .fruite-name a::before {
    content: '\f105';
    font-family: 'Font Awesome 5 Free';
    font-weight: 900;
    position: absolute;
    left: 0;
    top: 0;
    color: var(--secondary-color);
    transition: all 0.3s ease;
}

.fruite .fruite-categorie .fruite-name a:hover {
    color: var(--secondary-color);
    padding-left: 30px;
}

.fruite .fruite-categorie .fruite-name a:hover::before {
    left: 5px;
}

.fruite .fruite-item {
    height: 100%;
    transition: all 0.5s ease;
    border-radius: 15px;
    overflow: hidden;
    background: white;
    box-shadow: 0 10px 30px rgba(0, 0, 0, 0.05);
}

.fruite .fruite-item:hover {
    transform: translateY(-10px);
    box-shadow: 0 15px 40px rgba(0, 0, 0, 0.1);
}

.fruite .fruite-item .fruite-img {
    overflow: hidden;
    position: relative;
    border-radius: 15px 15px 0 0;
}

.fruite .fruite-item .fruite-img img {
    transition: all 0.5s ease;
    width: 100%;
}

.fruite .fruite-item:hover .fruite-img img {
    transform: scale(1.1);
}

.fruite .fruite-item .fruite-img .fruite-overlay {
    position: absolute;
    top: 0;
    left: 0;
    width: 100%;
    height: 100%;
    display: flex;
    align-items: center;
    justify-content: center;
    background: rgba(0, 0, 0, 0.2);
    opacity: 0;
    transition: all 0.5s ease;
}

.fruite .fruite-item:hover .fruite-img .fruite-overlay {
    opacity: 1;
}

.fruite .fruite-item .fruite-img .fruite-overlay a {
    width: 50px;
    height: 50px;
    display: inline-flex;
    align-items: center;
    justify-content: center;
    background: white;
    border-radius: 50%;
    margin: 0 5px;
    transform: translateY(30px);
    opacity: 0;
    transition: all 0.5s ease;
}

.fruite .fruite-item:hover .fruite-img .fruite-overlay a {
    transform: translateY(0);
    opacity: 1;
}

.fruite .fruite-item .fruite-img .fruite-overlay a:hover {
    background: var(--primary-color);
    color: white;
}

.fruite .fruite-item .fruite-content {
    padding: 20px;
}

.fruite .fruite-item .fruite-content h5 {
    font-size: 20px;
    font-weight: 700;
    margin-bottom: 10px;
}

.fruite .fruite-item .fruite-content .text-primary {
    color: var(--primary-color) !important;
}

.fruite .fruite-item .fruite-content .text-secondary {
    color: var(--secondary-color) !important;
}

.fruite .fruite-item .fruite-content .d-flex {
    margin-top: 15px;
}

.fruite .fruite-item .fruite-content .btn {
    font-weight: 700;
    transition: all 0.4s ease;
}

.fruite .fruite-item .fruite-content .btn:hover {
    background: var(--secondary-color);
    color: white !important;
}

.fruite .fruite-item .fruite-content .btn:hover i {
    color: white !important;
}
/*** Fruits End ***/


/*** vesitable Start ***/
.vesitable .vesitable-item {
    height: 100%;
    transition: 0.5s;
}

.vesitable .vesitable-item:hover {
    box-shadow: 0 0 55px rgba(29, 21, 21, 0.4);
}

.vesitable .vesitable-item .vesitable-img {
    overflow: hidden;
    transition: 0.5s;
    border-radius: 10px 10px 0 0;
}

.vesitable .vesitable-item .vesitable-img img {
    transition: 0.5s;
}

.vesitable .vesitable-item .vesitable-img img:hover {
    transform: scale(1.2);
}

.vesitable .owl-stage {
    margin: 50px 0;
    position: relative;
}

.vesitable .owl-nav .owl-prev {
    position: absolute;
    top: -8px;
    right: 0;
    color: var(--bs-primary);
    padding: 5px 25px;
    border: 1px solid var(--bs-secondary);
    border-radius: 20px;
    transition: 0.5s;

}

.vesitable .owl-nav .owl-prev:hover {
    background: var(--bs-secondary);
    color: var(--bs-white);
}

.vesitable .owl-nav .owl-next {
    position: absolute;
    top: -8px;
    right: 88px;
    color: var(--bs-primary);
    padding: 5px 25px;
    border: 1px solid var(--bs-secondary);
    border-radius: 20px;
    transition: 0.5s;
}

.vesitable .owl-nav .owl-next:hover {
    background: var(--bs-secondary);
    color: var(--bs-white);
}
/*** vesitable End ***/


/*** Banner Section Start ***/
